{
	"$schema": "http://json.schemastore.org/prettierrc",
	"arrowParens": "always",
	"bracketSpacing": true,
	"jsxSingleQuote": false,
	"objectCurlySpacing": true,
	"quoteProps": "consistent",
	"printWidth": 100,
	"semi": false,
	"singleQuote": true,
	"tabWidth": 4,
	"trailingComma": "all",
	"useTabs": true,
	"endOfLine": "auto"
}
